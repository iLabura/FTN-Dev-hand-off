{
  "background": {
    "background-base": {
      "value": "$grey.grey-90",
      "type": "color"
    },
    "gm-background-base": {
      "value": "$grey.grey-90",
      "type": "color"
    },
    "background-progress-bar": {
      "value": "$grey.grey-85",
      "type": "color",
      "description": "To be used on progress-bar backgrounds."
    }
  },
  "surface": {
    "surface-primary": {
      "value": "$base.white",
      "type": "color"
    },
    "surface-secondary": {
      "value": "$grey.grey-95",
      "type": "color"
    },
    "surface-panel": {
      "value": "$base.white",
      "type": "color"
    },
    "surface-action-regular": {
      "value": "$base.white",
      "type": "color"
    },
    "surface-action-regular--hover": {
      "value": "$grey.grey-95",
      "type": "color"
    },
    "gradient": {
      "value": "linear-gradient(0deg, $grey.grey-95 0%, $base.white 100%);",
      "type": "color"
    },
    "surface-inverted-bw": {
      "value": "$base.white",
      "type": "color"
    },
    "surface-input-inactive": {
      "value": "$grey.grey-95",
      "type": "color"
    },
    "surface-input-active": {
      "value": "$grey.grey-95",
      "type": "color"
    },
    "surface-input-text-inactive": {
      "value": "$grey.grey-90",
      "type": "color"
    },
    "surface-nav": {
      "value": "$base.white",
      "type": "color"
    },
    "transparent": {
      "primary-70": {
        "value": "{surface.surface-primary}",
        "type": "color",
        "$extensions": {
          "studio.tokens": {
            "modify": {
              "type": "alpha",
              "value": "0.7",
              "space": "lch"
            }
          }
        }
      }
    }
  },
  "content": {
    "content-primary": {
      "value": "$base.black",
      "type": "color"
    },
    "content-secondary": {
      "value": "$grey.grey-30",
      "type": "color"
    },
    "content-tertiary": {
      "value": "$grey.grey-50",
      "type": "color"
    },
    "content-on-content-primary": {
      "value": "$base.black",
      "type": "color"
    },
    "content-form-label-text": {
      "value": "$base.white",
      "type": "color"
    },
    "content-primary-inverted": {
      "value": "$base.white",
      "type": "color"
    },
    "content-perma-black": {
      "value": "$base.black",
      "type": "color"
    },
    "content-perma-white": {
      "value": "$base.white",
      "type": "color"
    },
    "content-disabled": {
      "value": "$grey.grey-60",
      "type": "color"
    }
  },
  "border": {
    "border-primary": {
      "value": "$grey.grey-90",
      "type": "color"
    },
    "border-secondary": {
      "value": "$grey.grey-85",
      "type": "color"
    },
    "border-tertiary": {
      "value": "$grey.grey-95",
      "type": "color"
    },
    "gm-border-base": {
      "value": "$grey.grey-80",
      "type": "color"
    },
    "border-btn-disabled": {
      "value": "$grey.grey-50",
      "type": "color"
    },
    "border-input": {
      "value": "$grey.grey-40",
      "type": "color"
    }
  },
  "overlay": {
    "overlay-scrim": {
      "value": "rgba($base.black,$opacity-scrim)",
      "type": "color"
    },
    "overlay-gradient": {
      "value": "linear-gradient(0deg, rgba(0,0,0,0.4) 0%, rgba(0,0,0,0) 100%)",
      "type": "color"
    },
    "gradient-primary-0deg-70-100": {
      "value": "linear-gradient(0deg, {surface.transparent.primary-70} 0%, {surface.surface-primary} 100%);",
      "type": "color"
    }
  },
  "color": {
    "overlay": {
      "navigation": {
        "value": "rgba({surface.surface-primary} , 0.8)",
        "type": "color"
      },
      "tooltip-default": {
        "value": "rgba( {grey.grey-30} , 0.8)",
        "type": "color"
      },
      "error": {
        "value": "rgba( {meaning.negative} , 0.8)",
        "type": "color"
      },
      "info": {
        "value": "rgba( {meaning.info} , 0.8)",
        "type": "color"
      },
      "positive": {
        "value": "rgba( {meaning.positive} , 0.8)",
        "type": "color"
      }
    }
  },
  "feedback": {
    "tooltip": {
      "default": {
        "value": {
          "fill": "{color.overlay.tooltip-default}",
          "backgroundBlur": "8",
          "borderRadius": "{radius-small}",
          "boxShadow": "{outer.shadow-outer-large}",
          "verticalPadding": "{space-xsmall}",
          "horizontalPadding": "{space-mezzo-6}"
        },
        "type": "composition"
      },
      "error": {
        "value": {
          "fill": "{color.overlay.error}",
          "backgroundBlur": "8",
          "borderRadius": "{radius-small}",
          "boxShadow": "{outer.shadow-outer-large}"
        },
        "type": "composition"
      },
      "info": {
        "value": {
          "fill": "{color.overlay.info}",
          "backgroundBlur": "8",
          "borderRadius": "{radius-small}",
          "boxShadow": "{outer.shadow-outer-large}"
        },
        "type": "composition"
      }
    },
    "snackbar": {
      "alert": {
        "value": {
          "fill": "{color.overlay.error}",
          "horizontalPadding": "{space-large}",
          "verticalPadding": "{space-mezzo-12}",
          "borderRadius": "{radius-medium}",
          "boxShadow": "{outer.shadow-outer-large}",
          "backgroundBlur": "12"
        },
        "type": "composition"
      },
      "info": {
        "value": {
          "fill": "{color.overlay.info}",
          "horizontalPadding": "{space-large}",
          "verticalPadding": "{space-mezzo-12}",
          "borderRadius": "{radius-medium}",
          "boxShadow": "{outer.shadow-outer-large}",
          "backgroundBlur": "12"
        },
        "type": "composition"
      },
      "positive": {
        "value": {
          "fill": "{color.overlay.positive}",
          "horizontalPadding": "{space-large}",
          "verticalPadding": "{space-mezzo-12}",
          "borderRadius": "{radius-medium}",
          "boxShadow": "{outer.shadow-outer-large}",
          "backgroundBlur": "12"
        },
        "type": "composition"
      }
    }
  },
  "special-surface": {
    "navigation-bar": {
      "value": {
        "fill": "{color.overlay.navigation}",
        "backgroundBlur": "8"
      },
      "type": "composition"
    },
    "glass-gradient-primary-0deg": {
      "value": {
        "fill": "{overlay.gradient-primary-0deg-70-100}",
        "backgroundBlur": "10"
      },
      "type": "composition"
    }
  }
}